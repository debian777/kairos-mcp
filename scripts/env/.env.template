# Infra (compose, Keycloak, Postgres, Qdrant)
KEYCLOAK_ADMIN_PASSWORD=__KEYCLOAK_ADMIN_PASSWORD__
KEYCLOAK_ADMIN_USERNAME=admin
KEYCLOAK_DB_PASSWORD=__KEYCLOAK_DB_PASSWORD__
QDRANT_API_KEY=__QDRANT_API_KEY__
QDRANT_COLLECTION=kairos_dev
QDRANT_RESCORE=true
QDRANT_URL=http://localhost:6333

# App (single .env for dev and integration)
AUTH_ALLOWED_AUDIENCES=kairos-mcp
AUTH_CALLBACK_BASE_URL=http://localhost:3300
AUTH_ENABLED=true
AUTH_MODE=oidc_bearer
AUTH_TRUSTED_ISSUERS=http://localhost:8080/realms/kairos-dev
EMBEDDING_DIMENSION=1536
EMBEDDING_PROVIDER=openai
KAIROS_ENABLE_GROUP_COLLAPSE=false
KAIROS_REDIS_PREFIX=kairos:dev:
KAIROS_SEARCH_MAX_FETCH=200
KAIROS_SEARCH_OVERFETCH_FACTOR=1
KEYCLOAK_CLIENT_ID=kairos-mcp
KEYCLOAK_REALM=kairos-dev
KEYCLOAK_URL=http://localhost:8080
LOG_FORMAT=json
LOG_LEVEL=trace
METRICS_PORT=9390
NODE_ENV=development
OPENAI_API_KEY=__OPENAI_API_KEY__
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
PORT=3300
QDRANT_SNAPSHOT_DIR=./snapshots/dev/qdrant
QDRANT_SNAPSHOT_ON_START=true
REDIS_URL=redis://127.0.0.1:6379
SESSION_MAX_AGE_SEC=604800
SESSION_SECRET=__SESSION_SECRET__
